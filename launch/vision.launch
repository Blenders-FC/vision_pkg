<?xml version="1.0" ?>
<launch>    

  <!-- Define robot_id with a default value (used if not inherited) -->
  <arg name="robot_id" default="$(env ROBOT_ID)" doc="robot id [1, 2, 3, 4]"/>
  <param name="robot_id" value="$(arg robot_id)"/>

  <!-- Define quadrant with a default value (used if not inherited) -->
  <arg name="quadrant" default="3"/>
  <param name="quadrant" value="$(arg quadrant)"/>

  <!-- Camera Node -->
  <!-- <node pkg="usb_cam" type="usb_cam_node" name="usb_cam_node" output="screen"/> -->

  <!-- Vision Node -->
  <node pkg="vision_pkg" type="main_vision_node.py" name="main_vision_node"> <!-- output="screen" -->
    <param name="robot_id"               value="$(arg robot_id)"/>
    <param name="quadrant"               value="$(arg quadrant)"/>
  </node> 

  <!-- Referee Node -->
  <!-- <node pkg="vision_pkg" type="referee_node.py" name="referee_node" output="screen"/> -->


</launch>
